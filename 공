package 브3;

import java.util.Scanner;

public class 공 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int m = sc.nextInt();
		int z = 1;

		for (int i = 0; i < m; i++) {
			int x = sc.nextInt();
			int y = sc.nextInt();
			if (x == z)
				z = y;
			else if (y == z)
				z = x;
		}
		System.out.println(z);

	}

}
