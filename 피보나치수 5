package 재귀;

import java.util.*;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int sum = p(n);
		System.out.println(sum);

	}

	public static int p(int n) {
		if (n == 0)
			return 0;
		if (n == 1)
			return 1;
		return p(n - 1) + p(n - 2);

	}

}
