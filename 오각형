package 브3;

import java.util.Scanner;

public class 오각형 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);

		int n = sc.nextInt();
		long sum = 5;
		
		for (int i = 1; i < n; i++) 
			sum = (sum + (i+2)*3-2)%45678;
		
		System.out.println(sum);
		sc.close();
	}
}
